{
    "Routes": [
		{ 
			"DownstreamPathTemplate": "/api/{version}/sample",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host":"localhost",
					"Port": 8001
				}
			],
			"UpstreamPathTemplate": "/api/{version}/sample",
			"UpstreamHttpMethod":[ "POST", "GET", "OPTIONS" ],
			"SwaggerKey": "Samples"
		}, 
		{ 
			"DownstreamPathTemplate": "api/{version}/sample/{id}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host":"localhost",
					"Port": 8001
				}
			],
			"UpstreamPathTemplate": "/api/{version}/sample/{id}",
			"UpstreamHttpMethod":[ "GET", "PUT", "DELETE", "OPTIONS" ],
			"SwaggerKey": "Samples"
		}
    ],
    "SwaggerEndPoints": [
		{ 
			"Key": "Samples",
			"Config": [
				{
					"Name":"Sample Microservice API",
					"Version": "v1",
					"Url": "http://localhost:8001/swagger/v1/swagger.json"
				}
			]
		}
	]
}